schema1:Person a sh:NodeShape ;
    sh:closed true ;
    sh:description "A person (alive, dead, undead, or fictional)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:integer ;
            sh:description "Number of years since birth" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path hzb_metadata_schema:age_in_years ],
        [ sh:datatype xsd:string ;
            sh:description "The main email address of a person" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path schema1:email ],
        [ sh:datatype xsd:string ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path hzb_metadata_schema:has_employment_history ],
        [ sh:datatype xsd:string ;
            sh:description "Internationalized Resource Identifier" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path hzb_metadata_schema:iri ],
        [ sh:datatype xsd:string ;
            sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path schema1:name ],
        [ sh:in ( GSSO:000369 "Masculine" "Diverse" ) ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path schema1:gender ],
        [ sh:datatype xsd:date ;
            sh:description "Date on which a person is born" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path schema1:birthDate ],
        [ sh:datatype xsd:anyURI ;
            sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path schema1:identifier ] ;
    sh:targetClass schema1:Person .

