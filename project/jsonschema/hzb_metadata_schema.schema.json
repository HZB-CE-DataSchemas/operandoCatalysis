{
    "$defs": {
        "CatalysisSample": {
            "additionalProperties": false,
            "description": "core parameters of a catalysis sample #metadata terms (parameters) are mapped to nfdi voc4cat",
            "properties": {
                "age_in_years": {
                    "description": "Number of years since birth",
                    "type": "integer"
                },
                "birth_date": {
                    "description": "Date on which a person is born",
                    "format": "date",
                    "type": "string"
                },
                "primary_email": {
                    "description": "The main email address of a person",
                    "pattern": "^\\S+@[\\S+\\.]+\\S+",
                    "type": "string"
                },
                "sample_environment": {
                    "type": "string"
                },
                "vital_status": {
                    "$ref": "#/$defs/PersonStatus",
                    "description": "living or dead status"
                }
            },
            "title": "CatalysisSample",
            "type": "object"
        },
        "Device": {
            "additionalProperties": false,
            "description": "A thing made or adapted for a particular purpose, especially a piece of mechanical or electronic equipment",
            "title": "Device",
            "type": "object"
        },
        "NamedThing": {
            "additionalProperties": false,
            "description": "a databased entity or concept/class",
            "title": "NamedThing",
            "type": "object"
        },
        "PersonStatus": {
            "description": "",
            "enum": [
                "ALIVE",
                "DEAD",
                "UNKNOWN"
            ],
            "title": "PersonStatus",
            "type": "string"
        },
        "Sample": {
            "additionalProperties": false,
            "description": "Represents a Sample",
            "properties": {
                "age_in_years": {
                    "description": "Number of years since birth",
                    "type": "integer"
                },
                "birth_date": {
                    "description": "Date on which a person is born",
                    "format": "date",
                    "type": "string"
                },
                "primary_email": {
                    "description": "The main email address of a person",
                    "pattern": "^\\S+@[\\S+\\.]+\\S+",
                    "type": "string"
                },
                "vital_status": {
                    "$ref": "#/$defs/PersonStatus",
                    "description": "living or dead status"
                }
            },
            "title": "Sample",
            "type": "object"
        },
        "SampleCollection": {
            "additionalProperties": false,
            "description": "A holder for Sample objects",
            "properties": {
                "entries": {
                    "items": {
                        "$ref": "#/$defs/Sample"
                    },
                    "type": "array"
                }
            },
            "title": "SampleCollection",
            "type": "object"
        }
    },
    "$id": "https://w3id.org/anak-velazquez/hzb-metadata-schema",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "additionalProperties": true,
    "description": "A holder for Sample objects",
    "metamodel_version": "1.7.0",
    "properties": {
        "entries": {
            "items": {
                "$ref": "#/$defs/Sample"
            },
            "type": "array"
        }
    },
    "title": "hzb-metadata-schema",
    "type": "object",
    "version": null
}